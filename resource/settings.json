{
    "groups": [
        {
            "key": "base",
            "name": "Basic",
            "groups": [
                {
                    "key": "update",
                    "name": "Update",
                    "options": [
                        {
                            "key": "type",
                            "name": "Type",
                            "type": "combobox",
                            "default": "lastest"
                        }
                    ]
                },
                {
                    "key": "notification",
                    "name": "Notification",
                    "options": [
                        {
                            "key": "enable",
                            "name": "",
                            "type": "checkbox",
                            "text":"Send Notification",
                            "default": "true"
                        }
                    ]
                },
                {
                    "key": "domain",
                    "name": "Domain",
                    "options": [
                        {
                            "key": "subdomain",
                            "name": "Subdomain for bing.com",
                            "type": "combobox",
                            "default": "auto"
                        }
                    ]
                },
                {
                    "key": "autoupdate",
                    "name": "Auto Update",
                    "options": [
                        {
                            "key": "startup-enable",
                            "name": "",
                            "type": "checkbox",
                            "text": "Autoupdate Bingwallpaper when tool startup",
                            "default": "false"
                        },
                        {
                            "key": "interval",
                            "name": "Interval",
                            "type": "combobox",
                            "default": "-1"
                        }
                    ]
                },
                {
                    "key": "security",
                    "name": "Security",
                    "options": [
                        {
                            "key": "python-check",
                            "name": "",
                            "type": "checkbox",
                            "text": "Check setting Python file's MD5",
                            "default": "true"
                        }
                    ]
                }
            ]
